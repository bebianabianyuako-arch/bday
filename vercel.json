{
    "version": 2,
    "builds": [
        {
            "src": "birthday_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "birthday_project/wsgi.py"
        }
    ]
}